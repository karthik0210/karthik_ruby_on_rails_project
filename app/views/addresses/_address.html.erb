<div id="<%= dom_id address %>">
  <p>
    <strong>Name:</strong>
    <%= address.name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= address.phone %>
  </p>

  <p>
    <strong>Address line1:</strong>
    <%= address.address_line1 %>
  </p>

  <p>
    <strong>Address line2:</strong>
    <%= address.address_line2 %>
  </p>

  <p>
    <strong>City:</strong>
    <%= address.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= address.state %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= address.country %>
  </p>

  <p>
    <strong>Pin code:</strong>
    <%= address.pin_code %>
  </p>

</div>
